<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MobiBench Web Service</display-name>
  <servlet>
    <display-name>MobiBenchServlet</display-name>
    <servlet-name>MobiBenchServlet</servlet-name>
    <servlet-class>wvw.mobibench.service.servlet.MobiBenchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MobiBenchServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>CORS</filter-name>
    <filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>
    <init-param>
      <param-name>cors.supportedMethods</param-name>
      <param-value>HEAD, OPTIONS, GET, POST, PUT, DELETE</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CORS</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
</web-app>